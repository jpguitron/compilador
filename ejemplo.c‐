int can (int a[], int p[])
{
    output(a[0]);
    output(p[0]);
    p[0] = a[0]+p[0];
    return p[0];
}

void perro(int a[],int b)
{
    /*a[0]=79;
    a[10]=79;*/
    a[0]=69;
}
void main(void)
{
    int k;
    int p[3];

    p[0] =1000;
    perro(p,100);

    p[0] = can(p,p);
    output(p[0]+100);
    output(p[0]);
}
