/* Un programa para realizar el algoritmo
de Euclides para calcular mod. */
int a[10];
int gdc (int u, int v)
{
    
    if ( v == 0)
    {
        if ( v == 0)
        {
        }
        else
        {
            if ( v != 0)
            {
                int aaaaaaa;
            }
            else
            {
                int zzzzzzzz;
            }
        }
    }
    else return gdc(v,5-u/v*v/v);
    /* u-u/v*v == u mod v */
}

void main(void)
{
     int x; int y;
     x = input(); y = input();
     output(gdc(x,y));
}


